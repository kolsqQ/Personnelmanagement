<Window x:Class="Personnelmanagement.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Personnelmanagement"
        mc:Ignorable="d" Height="400" Width="600" ResizeMode="NoResize">
    <Grid>
        <Frame Margin="170,0,0,0" Name="appframe" NavigationUIVisibility="Hidden" Navigated="appframe_Navigated" />
        <StackPanel HorizontalAlignment="Left" VerticalAlignment="Center"   >
            <Button Name="Main" Content="Главная страница" HorizontalAlignment="Left" Width="150" Margin="10" Height="50" Click="MainPage_Click"/>
            <Button Name="EmployeeList" Content="Список сотрудников" HorizontalAlignment="Left" VerticalAlignment="Center" Width="150" Margin="10" Height="50" Click="EmployeeList_Click"/>
        </StackPanel>
    </Grid>
</Window>
